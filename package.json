{
  "name": "n8n-nodes-outplay",
  "version": "0.2.6",
  "description": "n8n nodes for Outplay API integration",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/outplay-hq/n8n-nodes-outplay",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "outplay",
    "sales",
    "engagement",
    "crm"
  ],
  "author": {
    "name": "Outplay",
    "email": "pratik@outplay.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/outplay-hq/n8n-nodes-outplay.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "dev:cli": "n8n-node dev",
    "dev:external": "n8n-node dev --external-n8n",
    "dev:n8n:unix": "bash ./start-dev.sh",
    "test": "npm run build && npm run lint",
    "prepack": "npm run clean && npm run build",
    "start:n8n": "set N8N_DEV_RELOAD=true && n8n start",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OutplayApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Outplay/Outplay.node.js",
      "dist/nodes/Outplay/OutplayTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.14.0",
    "@types/connect": "^3.4.38",
    "@types/estree": "^1.0.8",
    "@types/json-schema": "^7.0.15",
    "@types/luxon": "^3.7.1",
    "@types/mysql": "^2.15.27",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/pg-pool": "^2.0.6",
    "@types/retry": "^0.12.5",
    "@types/semver": "^7.7.1",
    "@types/shimmer": "^1.2.0",
    "@types/tedious": "^4.0.14",
    "@types/triple-beam": "^1.3.5",
    "@types/uuid": "^10.0.0",
    "eslint": "9.32.0",
    "n8n-node-dev": "^1.118.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.117.0"
  }
}
